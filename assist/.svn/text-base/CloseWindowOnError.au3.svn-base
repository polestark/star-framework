#include <WinAPI.au3>

$winName = "EGIS - 保全信息"
_IE($winName)

Func _IE($winName)
	Local $aWindows, $i, $text
	$aWindows = _WinAPI_EnumWindowsTop()
	For $i = 1 To $aWindows[0][0]
		If	$aWindows[$i][1] = "IEFrame" Then
			$text = WinGetTitle($aWindows[$i][0])
			$text = StringReplace($text, " - Windows Internet Explorer", "")
			If	$text = $winName Then
				WinClose($text)
				ExitLoop
			EndIf
		EndIf
	Next
	return ($text)
EndFunc